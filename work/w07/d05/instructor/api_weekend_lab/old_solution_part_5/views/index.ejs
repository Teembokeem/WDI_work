<!DOCTYPE html>
<html lang="en">
  <%- include('_head'); %>
<body>
  <%- include('_header'); %>

  <h3>Search <%= (Math.random() > 0.5) ? "Foursquare" : "Swarm" %></h3>
  <form method="GET" action="/venues">
    <label for="place" style="display: inline-block; width: 80px">Place Name</label>
    <input id="place" type="text" name="place[name]" placeholder="Cleveland, OH"/>
    <br>
    <label for="query" style="display: inline-block; width: 80px">Search For</label>
    <input id="query" type="text" name="place[query]" placeholder="hot dogs"/>
    <input type="submit" value="Search" />
  </form>

</body>
</html>
